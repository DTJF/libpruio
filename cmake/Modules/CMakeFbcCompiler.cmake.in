SET(CMAKE_Fbc_IMPLICIT_LINK_LIBRARIES @CMAKE_Fbc_IMPLICIT_LINK_LIBRARIES@)

SET(CMAKE_Fbc_SOURCE_FILE_EXTENSIONS bas;BAS)
