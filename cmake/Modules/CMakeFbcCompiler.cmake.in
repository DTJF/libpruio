SET(CMAKE_Fbc_COMPILER "@CMAKE_Fbc_COMPILER@")
SET(CMAKE_Fbc_COMPILER_ARG1 "@CMAKE_Fbc_COMPILER_ARG1@")
SET(CMAKE_Fbc_SIZEOF_ANY_PTR "@CMAKE_Fbc_SIZEOF_DATA_PTR@")
SET(CMAKE_AR "@CMAKE_AR@")
SET(CMAKE_RANLIB "@CMAKE_RANLIB@")
SET(CMAKE_COMPILER_IS_MINGW @CMAKE_COMPILER_IS_MINGW@)
SET(CMAKE_COMPILER_IS_CYGWIN @CMAKE_COMPILER_IS_CYGWIN@)
IF(CMAKE_COMPILER_IS_CYGWIN)
  SET(CYGWIN 1)
  SET(UNIX 1)
ENDIF()

IF(CMAKE_COMPILER_IS_MINGW)
  SET(MINGW 1)
ENDIF()

SET(CMAKE_Fbc_COMPILER_LOADED 1)
SET(CMAKE_Fbc_COMPILER_ENV_VAR "FBC")
SET(CMAKE_Fbc_COMPILER_ID "@CMAKE_Fbc_COMPILER_ID@")
SET(CMAKE_Fbc_IMPLICIT_LINK_DIRECTORIES @CMAKE_Fbc_IMPLICIT_LINK_DIRECTORIES@)
SET(CMAKE_Fbc_IMPLICIT_LINK_LIBRARIES @CMAKE_Fbc_IMPLICIT_LINK_LIBRARIES@)

SET(CMAKE_Fbc_SOURCE_FILE_EXTENSIONS bas;BAS)
SET(CMAKE_Fbc_IGNORE_EXTENSIONS bi;BI;o;O;obj;OBJ;rc;RC;def;DEF;h;hp)
SET(CMAKE_Fbc_LINKER_PREFERENCE None)
IF(UNIX)
  SET(CMAKE_Fbc_OUTPUT_EXTENSION .o)
ELSE()
  SET(CMAKE_Fbc_OUTPUT_EXTENSION .obj)
ENDIF()

IF(NOT CMAKE_SIZEOF_VOID_P)
  SET(CMAKE_SIZEOF_VOID_P "@CMAKE_Fbc_SIZEOF_DATA_PTR@")
ENDIF()
